doctype html
html
  head
    link(rel="shortcut icon", href="images/watermarkyellow.png")
    link(href="css/style.css", rel="stylesheet")
    title LOGIN
  body
    div.container
      h1 Log in to view!
      form(method="post", onsubmit="hashInput('password', 'username')")
        p Username
        input(type="text", name="username", id="username")
        p Password
        input(type="password", name="password", id="password")
        button Submit
  script(src="js/formCalls.js")
